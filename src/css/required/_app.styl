body {
  margin: 0;
  padding: 0;
  font-family: sans-serif;
}

.App {
  text-align: center;
}

.App-logo {
  animation: App-logo-spin infinite 20s linear;
  height: 80px;
}

.App-header {
  background-color: #222;
  height: 150px;
  padding: 20px;
  color: white;
}

.App-title {
  font-size: 1.5em;
}

.App-intro {
  font-size: large;
}

@keyframes App-logo-spin {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}

@-webkit-keyframes liner-tab {
/* Chrome, Safari */
  0% { width: 100%; }
  40% { width: 0%; }
  60% { width: 0%; }
  100% { width: 0%; }
}
@keyframes liner-tab {
  0% { width: 100%; }
  40% { width: 0%; }
  60% { width: 0%; }
  100% { width: 0%; }
}